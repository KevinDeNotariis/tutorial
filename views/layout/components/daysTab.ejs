<div class="list-group" id="list-tab" role="tablist">
    <% daysPlusExercises.map((elem,key) => {%>
    <a
        class="list-group-item list-group-item-action <% if(key==0) { %> active <% }%>"
        id="list-<%= elem.day %>-list"
        data-toggle="list"
        href="#list-<%= elem.day %>"
        role="tab"
        aria-controls="<%= elem.day %>"
        ><%= elem.day %></a
    >
    <%})%>
</div>
